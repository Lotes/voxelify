{
  "name": "voxelify",
  "version": "1.0.0",
  "description": "This project can be used to transform a 3D model (OBJ file) into voxel information (3D pixels).",
  "homepage": "https://github.com/Lotes/voxelify#readme",
  "bugs": {
    "url": "https://github.com/Lotes/voxelify/issues"
  },
  "license": "MIT",
  "author": "Markus Rudolph",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lotes/voxelify.git"
  },
  "scripts": {
    "test": "standard && mocha --require should --reporter spec",
    "vox-test": "standard test/VoxelifierTests.spec.js.backup && mocha --require should --reporter spec test/VoxelifierTests.spec.js.backup",
    "uml": "node doc/uml/generate.js doc/uml",
    "generate-coverage": "istanbul cover ./node_modules/mocha/bin/_mocha --report html -- --require should -R spec",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- --require should -R spec && cat ./coverage/lcov.info | node ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "dependencies": {
    "bluebird": "^3.4.1",
    "callsite": "^1.0.0",
    "canvas": "^1.4.0",
    "get-installed-path": "^1.0.2",
    "gifencoder": "^1.0.6",
    "jsonpath-plus": "^0.16.0",
    "jsonschema": "^1.1.1",
    "jszip": "^3.1.3",
    "semver": "^5.3.0",
    "three": "^0.79.0"
  },
  "devDependencies": {
    "coveralls": "^2.11.12",
    "istanbul": "^0.4.5",
    "mocha": "^2.5.3",
    "mocha-lcov-reporter": "^1.2.0",
    "should": "^9.0.2",
    "standard": "^8.0.0"
  },
  "standard": {
    "ignore": [
      "lib/converters/OBJMTLLoader.js",
      "lib/shared/node-three/LoadingManager.js"
    ]
  }
}
